{
	"plugins": [
		"prettier-plugin-organize-attributes",
		"prettier-plugin-tailwindcss"
	],
	"attributeSort": "ASC",
	"attributeGroups": [
		"^class$",
		"^(id|name)$",
		"$DEFAULT",
		"^aria-",
		"$ANGULAR_OUTPUT",
		"$ANGULAR_TWO_WAY_BINDING",
		"$ANGULAR_INPUT",
		"$ANGULAR_STRUCTURAL_DIRECTIVE"
	]
}
