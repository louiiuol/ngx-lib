{
  "cSpell.words": [
    "ngxlib"
  ],
  "css.customData": ["./tailwind.directives.json"],
  "eslint.validate": ["javascript", "typescript"],
  "eslint.workingDirectories": [
    {
      "changeProcessCWD": true,
      "directory": "./"
    }
  ],
  "eslint.useFlatConfig": false,
  "files.exclude": {
    "**/node_modules": true,
    "**/.DS_Store": true,
    "coverage": true,
    ".git/": true,
    ".angular/": true,
    "package-lock.json": true,
    "tailwind.directives.json": true,
    "angular.json": true,
    "jest.config.ts": true,
    "tsconfig.json": true,
    "tsconfig.app.json": true,
    "tsconfig.spec.json": true,
    ".editorconfig": true,
    "dist/": true,
    ".eslintrc.json": true,
    ".gitignore": true,
    ".prettierignore": true,
    ".prettierrc": true,
    ".github/": true,
    ".vscode/launch.json": true,
    ".vscode/tasks.json": true,
  },
  "files.watcherExclude": {
    "**/dist": true,
    "**/node_modules/*/**": true,
    ".git/": true,
    ".angular/": true,
  },
  "search.exclude": {
    "**/dist": true,
    "**/node_modules": true,
    ".git/": true,
    ".angular/": true,
    "**/*.d.ts": true,
    "**/*.d.ts.map": true
  },
}
